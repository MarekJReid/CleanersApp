<% include ../partials/header %>




<div class="container">
    <header class="jumbotron">
        <div class="container">
            <h1>Cleaner Profile Page!</h1>
            <p>Check out the guys and gals that will free up your life:</p>
            <p>Or click below to earn a bit of extra pocket money!</p>
            <a class="btn btn-primary" href="/cleanerProfiles/new">Join </a>
        </div>
    </header>


    <div class="row  text-center style: display: flex, flex-wrap: wrap;">
        <% cleaners.forEach(function(cleaner){ %>
        <div class="col-md-3 col-sm-6">
            <div class="thumbnail">
                <div class="caption">
                    <img class="img-thumbnail" src=" <%= cleaner.image %> " alt="" height='400px' width="400px">
                    <h4>
                        <%= cleaner.name %>
                    </h4>
                    <p>
                        <a href="/cleanerProfiles/<%= cleaner._id %>" class="btn btn-primary">More Info</a>
                    </p>
                </div>
            </div>
        </div>
        <% }) %>
    </div>
</div>

